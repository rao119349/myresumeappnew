{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "color-adjust"
        ]
      }
    ],
    "css-prefix-order": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ]
  }
}